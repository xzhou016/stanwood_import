{"components/search/show-more":"{{#if product_results.facets}}\n  {{> components/faceted-search/show-more-facets product_results}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<ul class=\"nav-list faceted-search-option-columns\" data-additional-facets-list>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"nav-list-item sidebar-filter-item filter-item\" data-facet-item>\n          <a href=\"{{url}}\"\n          class=\"nav-ist-action {{#if selected}}is-active{{/if}} sidebar-filter-item-link filter-item-link\"\n          rel=\"nofollow\"\n          data-id=\"{{ id }}\"\n          data-faceted-search-facet>{{title}} ({{count}})</a></li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}