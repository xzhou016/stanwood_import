{"pages/account/orders/invoice":"{{#partial \"head\"}}\r\n    <link href=\"{{cdn 'assets/css/invoice.css'}}\" rel=\"stylesheet\">\r\n{{/partial}}\r\n{{#partial \"page\"}}\r\n    <div class=\"main full\">\r\n        <aside class=\"page-sidebar\">\r\n            {{> components/common/sidebar}}\r\n        </aside>\r\n        <div class=\"main-content\">\r\n            <div class=\"Invoice\">\r\n                <h1>\r\n                    {{#if settings.store_logo.image}}\r\n                        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                    {{else}}\r\n                        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n                    {{/if}}\r\n                </h1>\r\n                <div class=\"InvoiceTitle\">\r\n                    {{lang 'invoice.for_order' name=settings.store_name id=order.id}}\r\n                </div>\r\n                <address><strong>{{nl2br settings.address}}</strong></address>\r\n                <div class=\"AddressRow\">\r\n                    <div class=\"BillingAddress\">\r\n                        <h3>{{lang 'account.orders.details.bill_to'}}</h3>\r\n                        <ul>\r\n                            <li><strong>{{order.billing_address.full_name}}</strong></li>\r\n                            <li>{{order.billing_address.company}}</li>\r\n                            <li>{{order.billing_address.address_lines.[0]}}</li>\r\n                            <li>{{order.billing_address.address_lines.[1]}}</li>\r\n                            <li>{{order.billing_address.city}}, {{order.billing_address.state}} {{order.billing_address.zip}}</li>\r\n                            <li>{{order.billing_address.country}}</li>\r\n                            <li>&nbsp;</li>\r\n                            <li>{{lang 'invoice.phone' number=order.billing_address.phone}}</li>\r\n                            {{#if order.billing_address.email}}<li>{{lang 'invoice.email' email=order.billing_address.email}}</li>{{/if}}\r\n                        </ul>\r\n                    </div>\r\n                    {{#if order.shipping_address}}\r\n                        {{#unless order.has_multiple_shipping_addresses}}\r\n                            <div class=\"ShippingAddress\">\r\n                                <h3>{{lang 'account.orders.details.ship_to'}}</h3>\r\n                                <ul>\r\n                                    <li><strong>{{order.shipping_address.full_name}}</strong></li>\r\n                                    <li>{{order.shipping_address.company}}</li>\r\n                                    <li>{{order.shipping_address.address_lines.[0]}}</li>\r\n                                    <li>{{order.shipping_address.address_lines.[1]}}</li>\r\n                                    <li>{{order.shipping_address.city}}, {{order.shipping_address.state}} {{order.shipping_address.zip}}</li>\r\n                                    <li>{{order.shipping_address.country}}</li>\r\n                                    <li>&nbsp;</li>\r\n                                    <li>{{lang 'invoice.phone' number=order.shipping_address.phone}}</li>\r\n                                    {{#if order.shipping_address.email}}<li>{{lang 'invoice.email' email=order.shipping_address.email}}</li>{{/if}}\r\n                                </ul>\r\n                            </div>\r\n                        {{/unless}}\r\n                    {{/if}}\r\n                </div>\r\n                <div class=\"InvoiceDetails\">\r\n                    <div class=\"InvoiceDetailsLeft\">\r\n                        <div class=\"DetailRow\">\r\n                            <div class=\"Label\">{{lang 'invoice.order'}}</div>\r\n                            <div class=\"Value\">#{{order.id}}</div>\r\n                        </div>\r\n                        <div class=\"DetailRow\">\r\n                            <div class=\"Label\">{{lang 'invoice.payment_method'}}</div>\r\n                            <div class=\"Value\">{{order.payment_method}} ({{order.total.formatted}})</div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"InvoiceDetailsRight\">\r\n                        <div class=\"DetailRow\">\r\n                            <div class=\"Label\">{{lang 'invoice.order_date'}}</div>\r\n                            <div class=\"Value\">{{order.date}}</div>\r\n                        </div>\r\n                        {{#if order.shipping_method}}\r\n                            <div class=\"DetailRow\">\r\n                                <div class=\"Label\">{{lang 'invoice.shipping_method'}}</div>\r\n                                <div class=\"Value\">{{order.shipping_method}}</div>\r\n                            </div>\r\n                        {{/if}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"InvoiceItems\">\r\n                    <h2 class=\"InvoiceHeading\">{{lang 'invoice.order_items'}}</h2>\r\n                    <table class=\"InvoiceTable\">\r\n                        <thead>\r\n                        <tr>\r\n                            {{#if order.has_multiple_shipping_addresses}}\r\n                                <th class=\"ProductAddress\">{{lang 'invoice.shipping_address'}}</th>\r\n                            {{/if}}\r\n                            <th class=\"ProductQuantity\">{{lang 'invoice.qty'}}</th>\r\n                            <th class=\"ProductSku\">{{lang 'invoice.code'}}</th>\r\n                            <th class=\"ProductDetails\">{{lang 'invoice.product_name'}}</th>\r\n                            <th class=\"ProductCost\" align=\"right\">{{lang 'invoice.price'}}</th>\r\n                            <th class=\"ProductTotal\" align=\"right\">{{lang 'invoice.total'}}</th>\r\n                        </tr>\r\n                        </thead>\r\n                        <tbody class=\"InvoiceItemList\">\r\n                        {{#each order.items}}\r\n                            <tr>\r\n                                {{#if ../order.has_multiple_shipping_addresses}}\r\n                                    <td class=\"ProductAddress\">\r\n                                        {{#each shipping_rows}}\r\n                                            <div>{{address}}</div>\r\n                                            <div>{{city}}, {{state}} {{zip}}</div>\r\n                                            <div>{{country}}</div>\r\n                                        {{/each}}\r\n                                    </td>\r\n                                {{/if}}\r\n                                <td class=\"ProductQuantity\">{{quantity}}</td>\r\n                                <td class=\"ProductSku\">{{sku}}</td>\r\n                                <td class=\"ProductDetails\">\r\n                                    {{name}}\r\n                                    <div class=\"ProductAttributes\">\r\n\r\n                                        {{#each options}}\r\n                                            <div class=\"ConfigurableProductRow\">\r\n                                                <div class=\"Label\">{{name}}:</div>\r\n                                                <div class=\"Value\"></div>\r\n                                                <div class=\"Value\">{{value}}</div>\r\n                                            </div>\r\n                                        {{/each}}\r\n                                    </div>\r\n                                    <div class=\"ProductConfigurableFields\">\r\n                                        {{#each customizations}}\r\n                                            <div class=\"ConfigurableProductRow\">\r\n                                                <div class=\"Label\">{{name}}:</div>\r\n                                                <div class=\"Value\"></div>\r\n                                                <div class=\"Value\">{{value}}</div>\r\n                                            </div>\r\n                                        {{/each}}\r\n                                    </div>\r\n                                    {{#if gift_wrapping_name}}\r\n                                        <div class=\"ProductGiftWrapping\">\r\n                                            {{gift_wrapping_name}}\r\n                                        </div>\r\n                                    {{/if}}\r\n                                    {{#if event_date}}\r\n                                        <div class=\"ProductEventDate\">\r\n                                            <div class=\"ConfigurableProductRow\">\r\n                                                <div class=\"Label\">{{event_date.name}}:</div>\r\n                                                <div class=\"Value\"></div>\r\n                                                <div class=\"Value\">{{event_date.date}}</div>\r\n                                            </div>\r\n                                        </div>\r\n                                    {{/if}}\r\n                                    {{#if pre_ordered}}\r\n                                        <div class=\"ProductPreOrder\">\r\n                                            <div class=\"ConfigurableProductRow\">\r\n                                                <div class=\"Label\">Preordered:</div>\r\n                                                <div class=\"Value\">{{lang 'common.yes'}}</div>\r\n                                            </div>\r\n                                        </div>\r\n                                    {{/if}}\r\n                                </td>\r\n                                <td class=\"ProductCost\">{{price.formatted}}</td>\r\n                                <td class=\"ProductTotal\">{{total.formatted}}</td>\r\n                            </tr>\r\n                        {{/each}}\r\n                        </tbody>\r\n                        <tfoot class=\"InvoiceTotals\">\r\n                        {{#each order.total_rows}}\r\n                            <tr class=\"InvoiceTotalRow\">\r\n                                <td class=\"Title\" colspan=\"{{#if ../order.has_multiple_shipping_addresses}}5{{else}}4{{/if}}\">{{label}}</td>\r\n                                <td class=\"Total\">{{value.formatted}}</td>\r\n                            </tr>\r\n                        {{/each}}\r\n                        </tfoot>\r\n                    </table>\r\n                    <div class=\"InvoiceComments\">\r\n                        <div class=\"InvoiceHeading\">{{lang 'invoice.comments'}}</div>\r\n                        <blockquote>\r\n                            {{order.comments}}\r\n                        </blockquote>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <script>\r\n                window.print();\r\n            </script>\r\n        </div>\r\n    </div>\r\n{{/partial}}\r\n{{> layout/empty}}","components/common/sidebar":"{{> components/common/side-categories}}\r\n{{> components/common/side-badges}}","layout/empty":"<!DOCTYPE html>\r\n<html>\r\n<head>\r\n    <title>{{ head.title }}</title>\r\n    {{{ head.meta_tags }}}\r\n    {{{ head.config }}}\r\n\r\n    <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n    {{#block \"head\"}}{{/block}}\r\n</head>\r\n<body>\r\n    {{#block \"page\"}}{{/block}}\r\n</body>\r\n</html>\r\n","components/common/side-categories":"<div class=\"side-categories\">\r\n    <h4>Department</h4>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n        <li class=\"navPages-item{{#if children}} has-children{{/if}}\">\r\n            {{> components/common/navigation-list-alternate}}\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>","components/common/side-badges":"<div class=\"sidebar-badges\">\r\n    <div id=\"promo1\" class=\"promo\">\r\n        <a href=\"http://www.facebook.com/pages/Stanwood-Imports/356428691049691\" target=\"_blank\">\r\n            <img align=\"left\" src=\"{{cdn 'assets/img/follow_us_on_facebook.jpg'}}\" alt=\"Follow Us on Facebook\" height=\"70\" width=\"125\" />\r\n        </a>\r\n    </div>\r\n    <div>\r\n        <a href=\"http://www.bbb.org/los-angeles/business-reviews/internet-shopping/stanwood-imports-in-chatsworth-ca-100095014\" target=\"_blank\"><img border=\"0\" alt=\"View our rating on BBB\" width=\"115\" height=\"63\" src=\"{{cdn 'assets/img/bbb-logo-A.png'}}\"></a>\r\n        <table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n            <tr>\r\n                <td width=\"135\" align=\"center\" valign=\"top\"><script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name=www.stanwoodimports.com&amp;size=S&amp;lang=en\"></script><br />\r\n                    <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <!-- END #promo1.promo -->\r\n    <div>    \r\n        <table width=\"137\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose Symantec SSL for secure e-commerce and confidential communications.\">\r\n            <tr>\r\n                <td width=\"135\" align=\"center\" valign=\"top\">\r\n                    <script type=\"text/javascript\" src=\"https://seal.websecurity.norton.com/getseal?host_name=www.stanwoodimports.com&amp;size=M&amp;use_flash=NO&amp;use_transparent=NO&amp;lang=en\"></script>\r\n                    <br />\r\n                    <a href=\"http://www.symantec.com/ssl-certificates\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\">ABOUT SSL CERTIFICATES</a>\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n</div>","components/common/navigation-list-alternate":"{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\r\n    </a>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item-parent\">\r\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item-child\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    {{> components/common/navigation-dropdown}}\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"}