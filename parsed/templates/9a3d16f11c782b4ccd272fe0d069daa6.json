{"components/common/share-buttons":"{{#if share_buttons.buttons.length}}\n  <nav class=\"share-buttons-nav\">\n    {{#if share_heading}}\n      <h2 class=\"share-heading\">{{lang 'common.share_this'}}</h2>\n    {{/if}}\n\n    <ul class=\"share-buttons\" data-share-buttons>\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\"\n            class=\"facebook\">\n              <svg class=\"icon-social\" title=\"Share on Facebook\" role=\"img\">\n                <use xlink:href=\"#icon-facebook\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              <svg class=\"icon-social\" title=\"Share on Twitter\" role=\"img\">\n                <use xlink:href=\"#icon-twitter\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\"\n            class=\"google-plus\">\n              <svg class=\"icon-social\" title=\"Share on Google Plus\" role=\"img\">\n                <use xlink:href=\"#icon-google-plus\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\"\n            class=\"linkedin\">\n              <svg class=\"icon-social\" title=\"Share on LinkedIn\" role=\"img\">\n                <use xlink:href=\"#icon-linkedin\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n            class=\"email\">\n              <svg class=\"icon-social\" title=\"Share by email\" role=\"img\">\n                <use xlink:href=\"#icon-mail\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              <svg class=\"icon-social\" title=\"Print page\" role=\"img\">\n                <use xlink:href=\"#icon-print\"></use>\n              </svg>\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest' }}\n          {{#if ../../image}}\n            <li class=\"social-link pinterest\">\n              <a\n                target=\"_blank\"\n                href=\"//pinterest.com/pin/create/button/?url={{../../../url}}{{#if ../../../image}}&amp;media={{../../../../image}}{{/if}}&amp;description={{../../../title}}\"\n                class=\"share-pinterest\"\n              >\n                <svg class=\"icon-social\" title=\"Share on Pinterest\" role=\"img\">\n                  <use xlink:href=\"#icon-pinterest\"></use>\n                </svg>\n              </a>\n            </li>\n          {{/if}}\n        {{/if}}\n      {{/each}}\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n      {{/each}}\n    </ul>\n  </nav>\n{{/if}}\n"}