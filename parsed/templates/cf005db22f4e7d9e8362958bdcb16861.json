{"components/cart/minicart":"<section class=\"minicart {{#unless cart.items.length}}minicart-is-empty{{/unless}}\" data-minicart>\n  <header class=\"minicart-header\">\n    <h1 class=\"minicart-title\">{{lang 'cart.minicart.title'}}</h1>\n\n    <button class=\"minicart-close\" data-minicart-close>\n      <svg class=\"minicart-close-icon\" title=\"Close minicart\" role=\"img\">\n        <use xlink:href=\"#icon-close\"></use>\n      </svg>\n    </button>\n  </header>\n  {{#if cart.items.length}}\n    {{> components/cart/minicart-content}}\n    {{> components/cart/minicart-footer}}\n  {{else}}\n    <div class=\"minicart-empty\">{{lang 'cart.minicart.empty'}}</div>\n  {{/if}}\n</section>\n","components/cart/minicart-content":"<main class=\"minicart-items\" data-minicart-content data-count=\"{{cart.quantity}}\">\n  {{#each cart.items}}\n    <div class=\"minicart-item\" data-minicart-item data-product-id=\"{{id}}\" data-current-count=\"{{quantity}}\">\n      <a href=\"{{url}}\" class=\"minicart-item-link\">\n        <div class=\"minicart-item-image\"\n             style=\"background-image: url({{getImage image 'small' (cdn ../theme_settings.default_image)}})\"></div>\n        <img class=\"sr-only\"\n             src=\"{{getImage image 'thumb' (cdn ../theme_settings.default-product-image)}}\"\n             alt=\"{{image.alt}}\"/>\n      </a>\n      <div class=\"minicart-item-info\">\n        <a href=\"{{url}}\">\n          <h2 class=\"minicart-item-title\">\n            {{name}}\n          </h2>\n        </a>\n\n        <div class=\"minicart-item-details\">\n          {{#each options}}\n            <div>{{name}}: {{value}}</div>\n          {{/each}}\n        </div>\n\n        <div class=\"minicart-item-quantity-price\">\n          {{> components/common/quantity this}}\n\n          <div class=\"minicart-item-price {{#if total_discounted.formatted}}minicart-item-price-discounted{{/if}}\">\n            <span class=\"minicart-item-price-regular\">{{total.formatted}}</span>\n            {{total_discounted.formatted}}\n          </div>\n        </div>\n\n        <a href=\"#\" data-minicart-item-remove>\n          {{lang 'cart.remove'}}\n        </a>\n      </div>\n    </div>\n  {{/each}}\n</main>\n","components/cart/minicart-footer":"<footer class=\"minicart-footer\">\n  {{#if cart.discount.value \"<\" 0}}\n    <div class=\"minicart-footer-row\">\n      <div class=\"minicart-footer-label\">\n        {{lang 'cart.minicart.discount'}}\n      </div>\n      <div class=\"minicart-footer-price\">\n        {{cart.discount.formatted}}\n      </div>\n    </div>\n  {{/if}}\n\n  <div class=\"minicart-footer-row\">\n    <div class=\"minicart-footer-label\">{{lang 'cart.minicart.total'}}</div>\n    <div class=\"minicart-footer-price\">{{cart.sub_total.formatted}}</div>\n  </div>\n\n  <a href=\"{{urls.cart}}\" class=\"button button-primary button-fullwidth button-minicart-viewcart\"><span>{{lang 'cart.minicart.view_cart'}}</span></a>\n  <a href=\"{{urls.checkout.single_address}}\" class=\"button button-secondary button-fullwidth\">{{lang 'cart.minicart.checkout'}}</a>\n</footer>\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"form-field-title\">{{lang 'product.quantity'}}</div>\n<span class=\"form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty[]\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <span class=\"form-quantity-arrow-wrapper form-quantity-arrow-up-wrapper\" data-quantity-control-action=\"increment\">\n    <svg class=\"form-quantity-arrow form-quantity-up\" title=\"Increase quantity\" role=\"img\">\n      <use xlink:href=\"#icon-lazy-arrow\"></use>\n    </svg>\n  </span>\n  <span class=\"form-quantity-arrow-wrapper form-quantity-arrow-down-wrapper\" data-quantity-control-action=\"decrement\">\n    <svg class=\"form-quantity-arrow form-quantity-down\" title=\"Decrease quantity\" role=\"img\">\n      <use xlink:href=\"#icon-lazy-arrow\"></use>\n    </svg>\n  </span>\n</span>\n"}