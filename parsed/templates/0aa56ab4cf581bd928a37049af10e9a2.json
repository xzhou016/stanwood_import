{"components/product/quick-view-modal":"<button class=\"modal-close\">\n  <svg class=\"modal-close-icon\" title=\"Close quick view\" role=\"img\">\n    <use xlink:href=\"#icon-close\"></use>\n  </svg>\n</button>\n\n<div class=\"modal-body quick-view-modal-body\" data-quick-view-product>\n  {{> components/product/images images=product.images}}\n  {{> components/product/details quick_view=true}}\n</div>\n","components/product/images":"<section class=\"product-image\">\n  {{#if images}}\n    <div class=\"product-image-main-image\" data-product-main-image>\n      {{#each images}}\n        <img\n          data-flickity-lazyload=\"{{getImage this 'medium' default_image=(cdn ../theme_settings.default_image)}}\"\n          class=\"product-main-image-slide\"\n          alt=\"{{alt}}\"\n          data-product-main-image-slide\n        />\n      {{/each}}\n\n      {{#if product.price.saved}}\n        <div class=\"product-item-tag {{prefix}}-item-tag tag-sale\">{{lang 'core.product.sale'}}</div>\n      {{/if}}\n    </div>\n\n    <div\n      class=\"product-image-thumbnails {{#if images.length '<' 2}} product-image-thumbnails-hidden{{/if}}\"\n      data-product-thumbnails\n    >\n      {{#each images}}\n        <figure\n          data-flickity-bg-lazyload=\"{{getImage this 'small' default_image=(cdn ../theme_settings.default_image)}}\"\n          class=\"product-thumbnails-slide\"\n          data-product-thumbnails-slide\n        >\n        </figure>\n      {{/each}}\n    </div>\n\n    <button\n      class=\"product-thumbnails-arrow product-thumbnails-arrow-previous {{#if images.length '<' 2}} product-thumbnails-arrow-hidden{{/if}}\"\n      type=\"button\"\n      data-carousel-slide-previous\n    >\n      <svg class=\"product-thumbnails-arrow-icon product-thumbnails-arrow-icon-previous\" title=\"Previous product image\" role=\"img\">\n        <use xlink:href=\"#icon-arrow\"></use>\n      </svg>\n    </button>\n    <button\n      class=\"product-thumbnails-arrow product-thumbnails-arrow-next {{#if images.length '<' 2}} product-thumbnails-arrow-hidden{{/if}}\"\n      type=\"button\"\n      data-carousel-slide-next\n    >\n      <svg class=\"product-thumbnails-arrow-icon product-thumbnails-arrow-icon-next\" title=\"Next product image\" role=\"img\">\n        <use xlink:href=\"#icon-arrow\"></use>\n      </svg>\n    </button>\n\n  {{else}}\n    <div class=\"product-image-main-image\" data-product-main-image>\n      <img\n        src=\"{{cdn theme_settings.default_image}}\"\n        class=\"product-main-image-slide\"\n        alt=\"{{alt}}\"\n        data-product-main-image-slide\n      />\n    </div>\n\n    <div class=\"product-image-thumbnails product-image-thumbnails-hidden\" data-product-thumbnails>\n      <figure\n        style=\"background-image: url('{{cdn theme_settings.default_image}}')\"\n        class=\"product-thumbnails-slide\"\n        data-product-thumbnails-slide\n      >\n      </figure>\n    </div>\n  {{/if}}\n\n  <div class=\"product-image-zooms\" data-product-image-zooms>\n    {{#if images}}\n      {{#each images}}\n        <div class=\"image-zoom\" data-image=\"{{getImage this 'zoom' default_image=(cdn ../theme_settings.default_image)}}\"></div>\n      {{/each}}\n    {{else}}\n      <div class=\"image-zoom\" data-image=\"{{cdn theme_settings.default_image}}\" alt=\"{{product.name}}\"></div>\n    {{/if}}\n  </div>\n</section>\n","components/product/details":"<section\n  class=\"product-details\"\n  data-product-details\n  data-product-title=\"{{product.title}}\"\n>\n  <form\n    class=\"form\"\n    method=\"post\"\n    action=\"{{product.cart_url}}\"\n    enctype=\"multipart/form-data\"\n    data-cart-item-add\n    {{#if customer}}data-is-customer{{/if}}\n    data-validated-form\n  >\n    <input type=\"hidden\" name=\"action\" value=\"add\">\n    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id>\n\n    <div class=\"product-details-top\">\n      <div class=\"product-details-rating\">\n        {{#if settings.show_product_rating}}\n          {{> components/reviews/ratings rating=product.rating}}\n        {{/if}}\n        {{#if settings.show_product_reviews}}\n          <a\n            href=\"{{#if quick_view}}{{product.url}}{{/if}}#reviews\"\n            class=\"review-link\"\n            {{#unless quick_view}}\n            data-scroll=\"#reviews\"\n            data-reviews-link\n            {{/unless}}\n          >\n            {{lang 'product.reviews.review_count' numReviews=product.num_reviews}}\n          </a>\n        {{/if}}\n      </div>\n\n      {{#if product.brand}}\n        <a href=\"{{product.brand.url}}\" class=\"product-brand\">{{product.brand.name}}</a>\n      {{/if}}\n\n      {{#unless quick_view}}\n        {{#each product.reviews.messages}}\n          {{#if error}}\n            {{> core/alerts/alert-error message=error dismissable=true}}\n          {{/if}}\n          {{#if success}}\n            {{> core/alerts/alert-success message=success dismissable=true}}\n          {{/if}}\n        {{/each}}\n      {{/unless}}\n\n      <h1 class=\"product-title\">{{product.title}}</h1>\n\n      <div class=\"product-details-price\">\n        {{#if product.call_for_price}}\n          {{product.call_for_price}}\n        {{/if}}\n\n        {{#if product.price}}\n          {{> components/product/price\n            price=product.price\n            show_savings=true\n          }}\n        {{/if}}\n      </div>\n\n      {{#if theme_settings.product-extra-details-view '===' 'description'}}\n        <div class=\"product-extra-details\">\n          {{#if product.sku}}\n            <div class=\"product-tab-details-item product-tab-details-sku\">\n              <span class=\"product-tab-details-item-name\">{{lang 'product.tabs.details.sku'}}</span>\n              <span class=\"product-tab-details-item-value\" data-product-sku>{{product.sku}}</span>\n            </div>\n          {{/if}}\n          {{#if product.upc}}\n            <div class=\"product-tab-details-item product-tab-details-upc\">\n              <span class=\"product-tab-details-item-name\">{{lang 'product.tabs.details.upc'}}</span>\n              <span class=\"product-tab-details-item-value\" data-product-upc>{{product.upc}}</span>\n            </div>\n          {{/if}}\n          {{#if product.gtin}}\n            <div class=\"product-tab-details-item product-tab-details-gtin\">\n              <span class=\"product-tab-details-item-name\">{{lang 'product.tabs.details.gtin'}}</span>\n              <span class=\"product-tab-details-item-value\" data-product-sku>{{product.gtin}}</span>\n            </div>\n            {{/if}}\n            {{#if product.mpn}}\n            <div class=\"product-tab-details-item product-tab-details-mpn\">\n              <span class=\"product-tab-details-item-name\">{{lang 'product.tabs.details.mpn'}}</span>\n              <span class=\"product-tab-details-item-value\" data-product-upc>{{product.mpn}}</span>\n            </div>\n          {{/if}}\n        </div>\n      {{/if}}\n\n      {{#if product.description}}\n        <div class=\"product-details-description-top\">\n          <div\n            class=\"product-details-description-short has-excerpt\"\n            data-excerpt-length=\"220\"\n          >\n            {{{product.description}}}\n          </div>\n\n          {{#unless quick_view}}\n            {{#if product.description.length '>' 220}}\n              <a\n                class=\"product-description-read-more\"\n                href=\"#description\"\n                data-scroll=\"#description\"\n                data-description-link\n              >\n                {{lang 'product.read_more'}}\n              </a>\n            {{/if}}\n          {{/unless}}\n        </div>\n      {{/if}}\n\n      {{> components/product/options}}\n      {{> components/common/quantity product}}\n    </div>{{! Product Details Top}}\n\n    {{! container for appending cart-add errors}}\n    <div class=\"product-alert\" data-product-cart-message></div>\n    <div class=\"product-alert\" data-product-message></div>\n\n    {{#if product.release_date}}\n      {{> components/alerts/alert-info product.release_date}}\n    {{/if}}\n\n    <div class=\"product-add-to-cart form-field\" data-product-add>\n      {{#if product.can_purchase}}\n        {{#if product.out_of_stock}}\n          <input\n            class=\"button button-disabled\"\n            type=\"submit\"\n            value=\"{{lang 'product.sold_out'}}\"\n            disabled\n          >\n        {{else}}\n          {{#if product.pre_order}}\n            <input\n              class=\"button button-primary\"\n              type=\"submit\"\n              value=\"{{lang 'product.pre_order'}}\"\n              data-button-purchase\n              data-button-preorder\n            >\n          {{else}}\n            <input\n              class=\"button button-primary\"\n              type=\"submit\"\n              value=\"{{lang 'product.add_to_cart'}}\"\n              data-button-purchase\n            >\n          {{/if}}\n        {{/if}}\n        {{#if quick_view}}\n          <a class=\"product-view\" href=\"{{product.url}}\">\n            {{lang 'product.view_product'}}\n          </a>\n        {{/if}}\n      {{else}}\n        <input\n          class=\"button button-disabled\"\n          type=\"submit\"\n          value=\"{{lang 'product.unavailable'}}\"\n          disabled\n        >\n      {{/if}}\n      {{#if settings.account_creation_enabled}}\n        {{#if settings.show_wishlist}}\n          <div class=\"product-wishlist-add\" {{#if customer}}data-is-customer{{/if}}>\n            {{#if customer.wishlists.length}}\n              <div class=\"button-wishlist-add\" data-dropdown>\n                <a class=\"button button-secondary button-wishlist-add\" href=\"#\" data-dropdown-toggle>\n                  <svg class=\"star star-wishlist\" title=\"{{lang 'core.account.wishlists.add_item'}}\" role=\"img\">\n                    <use xlink:href=\"#icon-star\"></use>\n                  </svg>\n                </a>\n                <ul class=\"add-to-wishlists\" data-dropdown-panel>\n                  {{#each customer.wishlists}}\n                    <li>\n                      <a href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\" data-wishlist=\"{{view_url}}\">\n                        {{name}} ({{num_items}})\n                      </a>\n                    </li>\n                  {{/each}}\n                </ul>\n              </div>\n            {{else}}\n              <a class=\"button button-secondary button-wishlist-add\" href=\"{{product.add_to_wishlist_url}}\">\n                <svg class=\"star star-wishlist\" title=\"{{lang 'core.account.wishlists.add_item'}}\" role=\"img\">\n                  <use xlink:href=\"#icon-star\"></use>\n                </svg>\n              </a>\n            {{/if}}\n          </div>\n        {{/if}}\n      {{/if}}\n      <span class=\"spinner\"></span>\n    </div>\n\n  </form>\n</section>\n","components/reviews/ratings":"<div class=\"rating-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n      <svg class=\"rating-star rating-star-full\" role=\"presentation\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{else}}\n      <svg class=\"rating-star rating-star-empty\" role=\"presentation\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{/if}}\n  {{/for}}\n</div>\n","core/alerts/alert-error":"<div class=\"alert alert-error\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n","core/alerts/alert-success":"<div class=\"alert alert-success\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n","components/product/price":"<div>\n  {{#if price.without_tax}}\n    <span data-product-price-wrapper=\"without-tax\">\n      {{#if price.saved}}\n        <span class=\"price-retail\">{{price.rrp_without_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price {{#if price.saved}}price-with-retail{{/if}}\"> {{price.without_tax.formatted}}</span>\n\n      {{#if price.with_tax}}\n        <span class=\"price-label\"> {{lang 'product.excluding_tax'}}</span>\n      {{/if}}\n    </span>\n  {{/if}}\n\n  {{#if price.with_tax}}\n    <div data-product-price-wrapper=\"with-tax\">\n      {{#if price.saved}}\n        <span class=\"retail-price\">{{price.rrp_with_tax.formatted}}</span>\n      {{/if}}\n\n      <span class=\"price {{#if price.saved}}price-with-retail{{/if}}\"> {{price.with_tax.formatted}}</span>\n\n      {{#if price.without_tax}}\n        <span class=\"tax-label\"> {{lang 'product.including_tax'}}</span>\n      {{/if}}\n    </div>\n  {{/if}}\n\n  {{#if show_savings}}\n    {{#if price.saved}}\n      <span class=\"product-price-saved\" data-product-price-saved>\n        {{lang 'product.you_save'}} {{price.saved.formatted}}!\n      </span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/product/options":"<section class=\"product-options\">\n  {{! container for appending product-option-change errors}}\n  <div data-product-option-message></div>\n\n  {{#if product.options}}\n    <div data-product-option-change>\n      {{#each product.options}}\n        {{{dynamicComponent 'core/forms/options'}}}\n      {{/each}}\n    </div>\n  {{/if}}\n\n  {{#if product.customizations}}\n    <div data-product-option-change>\n      {{#each product.customizations}}\n        {{{dynamicComponent 'core/forms/customizations'}}}\n      {{/each}}\n    </div>\n  {{/if}}\n\n  {{#if product.event_date}}\n    {{> core/products/event}}\n  {{/if}}\n</section>\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"form-field-title\">{{lang 'product.quantity'}}</div>\n<span class=\"form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty[]\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <span class=\"form-quantity-arrow-wrapper form-quantity-arrow-up-wrapper\" data-quantity-control-action=\"increment\">\n    <svg class=\"form-quantity-arrow form-quantity-up\" title=\"Increase quantity\" role=\"img\">\n      <use xlink:href=\"#icon-lazy-arrow\"></use>\n    </svg>\n  </span>\n  <span class=\"form-quantity-arrow-wrapper form-quantity-arrow-down-wrapper\" data-quantity-control-action=\"decrement\">\n    <svg class=\"form-quantity-arrow form-quantity-down\" title=\"Decrease quantity\" role=\"img\">\n      <use xlink:href=\"#icon-lazy-arrow\"></use>\n    </svg>\n  </span>\n</span>\n","components/alerts/alert-info":"<div class=\"alert alert-info\">\n  {{#if dismissable}}<a href=\"#\" class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  {{#if message}}\n    {{{message}}}\n  {{else}}\n    {{{this}}}\n  {{/if}}\n</div>\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    