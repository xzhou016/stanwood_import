{"components/nav-category-item":"<li class=\"nav-submenu-item\"\n    id=\"nav-menu-category-{{id}}\"\n    data-menu-item\n    {{#each breadcrumbs}}\n      {{#if ../url '===' url}}\n        data-menu-item-breadcrumb\n      {{/if}}\n    {{/each}}\n>\n  <div class=\"nav-submenu-item-top-row\" data-nav-menu-item-row>\n    <a href=\"{{url}}\" class=\" nav-submenu-item-link\">\n      {{name}}\n    </a>\n\n    {{#if children}}\n        <button class=\"submenu-toggle\" data-submenu-toggle>\n          <svg class=\"submenu-toggle-icon\" title=\"Display submenu\" role=\"img\">\n            <use xlink:href=\"#icon-lazy-arrow\"></use>\n          </svg>\n        </button>\n      </div>\n      <ul class=\"nav-submenu\" data-submenu>\n        {{#each children}}\n          {{> components/nav-category-item breadcrumbs=../breadcrumbs}}\n        {{/each}}\n      </ul>\n    {{else}}\n      </div>\n    {{/if}}\n</li>\n"}