{"components/faceted-search/facets/rating":"<ul class=\"{{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li class=\"sidebar-filter-item filter-item\" data-facet-item>\n      <a href=\"{{url}}\" class=\"sidebar-filter-item-link {{#if selected}}sidebar-filter-item-link-selected{{/if}}\" data-faceted-search-facet>\n        {{> components/reviews/ratings rating=param_value}}<span class=\"rating-filter-text\">{{lang 'filter.rating_up'}}</span>\n        {{#if ../show_product_counts}}<span class=\"rating-star-count\" data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/reviews/ratings":"<div class=\"rating-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n      <svg class=\"rating-star rating-star-full\" role=\"presentation\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{else}}\n      <svg class=\"rating-star rating-star-empty\" role=\"presentation\">\n        <use xlink:href=\"#icon-star\">\n      </svg>\n    {{/if}}\n  {{/for}}\n</div>\n"}