{"components/faceted-search/facets/multi":"<ul class=\"{{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}} {{#if items_count '<' total_items}}has-additional-filters{{/if}}\"\n    id=\"filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-count=\"{{items_count}}\"\n    data-facet=\"{{facet}}\"\n    data-facet-filter-wrapper>\n\n  {{#each items}}\n    <li class=\"{{#if @index '>=' 5}}hidden{{/if}} sidebar-filter-item filter-item\" data-facet-item>\n      <a href=\"{{url}}\"\n         class=\"sidebar-filter-item-link filter-item-link {{#if selected}}sidebar-filter-item-link-selected{{/if}}\"\n         data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n\n{{#if total_items \">\" 4}}\n<a class=\"sidebar-filter-item-link-selected sidebar-filter-item-link filter-item-link\"\n   href=\"#filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n   data-show-more-facets\n>\n  <span class=\"toggle-link-text toggle-link-text-off\">{{lang 'filter.sort.show_more'}}</span>\n  <span class=\"toggle-link-text toggle-link-text-on\">{{lang 'filter.sort.show_less'}}</span>\n</a>\n{{/if}}"}