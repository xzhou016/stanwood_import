{"components/cart/shipping-calculator":"<div class=\"shipping-calculator\" data-shipping-calculator>\n  <form class=\"form\">\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-country\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.select_a_country'}}</span>\n        <span class=\"form-select-wrapper\">\n          <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n            <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n            {{#each countries}}\n              <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                {{name}}\n              </option>\n            {{/each}}\n          </select>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-state\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.select_a_state'}}</span>\n        <span class=\"form-select-wrapper\">\n          <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n            <option>{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n            {{#each states}}\n              <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                {{name}}\n              </option>\n            {{/each}}\n          </select>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-city\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.suburb_city'}}</span>\n        <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\">\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-zip\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.zip_postal_code'}}</span>\n        <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\">\n      </label>\n    </div>\n\n    <div class=\"form-actions\">\n      <button class=\"button button-primary\">{{lang 'cart.shipping_calculator.estimate_shipping'}}</button>\n    </div>\n  </form>\n\n  <div data-shipping-quotes></div>\n  <div data-shipping-errors></div>\n</div>\n"}