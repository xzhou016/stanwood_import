{"components/common/header":"<img class=\"main-bg-img\" src=\"{{cdn 'assets/img/bg.jpg'}}\" />\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.top 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <div class=\"container\">\r\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n        </a>\r\n        {{#if template '===' 'pages/home'}}\r\n            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                {{> components/common/store-logo}}\r\n            </h1>\r\n        {{else}}\r\n            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                {{> components/common/store-logo}}\r\n            </div>\r\n        {{/if}}\r\n        {{> components/common/topmenu}}\r\n        <div class=\"mobile-cart\">\r\n            <a href=\"{{urls.cart}}\">\r\n                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n            </a>\r\n        </div>\r\n        {{{region name=\"header_bottom\"}}}\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n</header>\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/common/topmenu":"<div class=\"topmenu-phone\">\r\n    <div class=\"topmenu\">\r\n        <ul class=\"ulFirst\">\r\n            <li class=\"cartimg\"><a href=\"{{urls.cart}}\">Shopping Cart <span></span></a></li>\r\n            <li>&nbsp;|&nbsp;</li>\r\n            <li class=\"ulsecondlast\">\r\n                <a href=\"{{urls.checkout}}\">Checkout</a>\r\n            </li>\r\n        </ul>\r\n        <ul class=\"ulsecond\">\r\n            {{#if customer}}\r\n            <li><a href=\"{{urls.account.index}}\" title=\"View My Account\">My Account</a></li>\r\n            {{else}}\r\n            <li><a href=\"{{urls.auth.login}}\" title=\"Login\">Log In</a></li>\r\n            <li><a href=\"{{urls.auth.create_account}}\">Register</a></li>\r\n            {{/if}}\r\n        </ul>\r\n    </div>\r\n    <div class=\"phoneIcon\">\r\n        <span>Call:</span><a href=\"tel:1-888-670-5570\">1-888-670-5570</a><br><span>Text:</span><a href=\"tel:1-310-962-9184\">1-310-962-9184</a>\r\n    </div>\r\n    {{> components/common/sloyalty_statusbar}}\r\n</div>","components/common/navigation-menu":"<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} mobile-cats\">\r\n        {{#each categories}}\r\n        <li class=\"navPages-item{{#if children}} has-children{{/if}}\">\r\n            {{> components/common/navigation-list-alternate}}\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        <li class=\"navPages-item\">\r\n            <a class=\"navPages-action\" href=\"{{urls.home}}\">Home</a>\r\n        </li>\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                 <li class=\"navPages-item navPages-item-page\">\r\n                     <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                 </li>\r\n             {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} mobile-account\">\r\n        {{#if customer}}\r\n        <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"{{urls.account.index}}\" title=\"My Account\">My Account</a></li>\r\n        <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"{{urls.auth.logout}}\" title=\"Logout\">Logout</a></li>\r\n        {{else}}\r\n        <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"{{urls.auth.login}}\" title=\"Login\">Login</a></li>\r\n        <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\" title=\"Create Account\">Create Account</a></li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n","components/common/sloyalty_statusbar":"<!--\r\nNOTE:\r\nThis file contains content used by S Loyalty (www.sloyalty.com) to power your loyalty program on your storefront for this theme.\r\nDO NOT edit or delete this file as it would lead to service interruptions. If you have any inquiries, please contact us at support@sloyalty.com.\r\n-->\r\n<script type=\"text/javascript\">\r\n     window.sloyalty = window.sloyalty || {};\r\n     window.sloyalty.shop = '{{settings.secure_base_url}}';\r\n     window.sloyalty.key = '6787bea0-90ef-4909-b025-adcaf2e78aef';\r\n     window.sloyalty.customer_id = '{{customer.id}}';\r\n     window.sloyalty.customer_email = '{{customer.email}}';\r\n</script>\r\n<script src=\"//s.sloyalty.com/site_media/static/dist/js/v2/sloyalty.bigcommerce.js\" async=\"true\"></script>\r\n<div class=\"sloyalty-loyalty-widget\" data-key=\"6787bea0-90ef-4909-b025-adcaf2e78aef\" data-shop=\"{{settings.secure_base_url}}\" data-customer=\"{{customer.id}}\" data-email=\"{{customer.email}}\"></div>\r\n<div class=\"sloyalty-referral-widget\" data-key=\"6787bea0-90ef-4909-b025-adcaf2e78aef\" data-shop=\"{{settings.secure_base_url}}\" data-customer=\"{{customer.id}}\" data-email=\"{{customer.email}}\"></div>\r\n<div class=\"sloyalty-standalone-share-widget\" data-key=\"6787bea0-90ef-4909-b025-adcaf2e78aef\" data-shop=\"{{settings.secure_base_url}}\" data-customer=\"{{customer.id}}\" data-email=\"{{customer.email}}\"></div>","components/common/navigation-list-alternate":"{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\r\n    </a>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n","components/common/quick-search":"<div class=\"quicksearch-container\">\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                <input class=\"form-submit\" type=\"submit\" value=\"\" name=\"Search\" title=\"Search\">\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" style=\"display: none;\" data-bind=\"html: results\"></section>\r\n</div>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item-parent\">\r\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item-child\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    {{> components/common/navigation-dropdown}}\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n"}