{"components/cart/shipping-quotes":"\n<form class=\"form shipping-quote-form\">\n  <ul>\n    {{#each quotes.shipping_methods}}\n      <li class=\"form-field form-inline\">\n        <label for=\"shipping-quote-{{id}}\">\n          <input id=\"shipping-quote-{{id}}\" class=\"form-input form-radio\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\" data-shipping-quote>\n          <span class=\"form-label-text\">{{provider_name}} {{cost.formatted}}</span>\n        </label>\n      </li>\n    {{/each}}\n\n  {{#each quotes.ups_shipping_methods}}\n    <li class=\"form-field form-inline\">\n      <label>\n        <input id=\"shipping-quote-{{id}}\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\">\n        <span>\n          {{#if logo_path}}\n            <img src=\"{{logo_path}}\" alt=\"{{description}}\">\n          {{/if}}\n          {{description}} - {{cost.formatted}}\n        </span>\n      </label>\n    </li>\n  {{/each}}\n  </ul>\n  <div class=\"form-actions\">\n    <input class=\"button button-primary\" type=\"submit\" value=\"{{lang 'cart.shipping_calculator.select'}}\">\n  </div>\n</form>\n"}