{"components/cart/gift-certificates":"{{inject 'giftCertificateInputEmpty' (lang 'cart.gift_certificates.error')}}\n\n<div data-gift-certificates>\n  <form class=\"form form-gift-certificate\" method=\"post\" action=\"{{urls.cart}}\" data-gift-certificate-form>\n    <input\n      class=\"form-input\"\n      id=\"couponcode\"\n      type=\"text\"\n      name=\"certcode\"\n      placeholder=\"{{lang 'cart.gift_certificates.enter_code'}}\"\n      data-gift-certificate-input\n    >\n    <button class=\"button button-secondary button-gift-certificate\" type=\"submit\">\n      <span class=\"cart-button-text\">{{lang 'cart.gift_certificates.submit'}}</span>\n        <svg class=\"cart-button-arrow\" title=\"{{lang 'cart.gift_certificates.submit'}}\" role=\"img\">\n          <use xlink:href=\"#icon-arrow\"></use>\n        </svg>\n    </button>\n    <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n  </form>\n</div>\n"}