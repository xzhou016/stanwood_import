{"components/footer/newsletter":"<form class=\"form subscription-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n  <div class=\"form-field form-inline\">\n    <input class=\"visually-hidden\" id=\"nl_first_name\" name=\"nl_first_name\" type=\"text\" value=\"bc\">\n    <label class=\"show-for-sr\" for=\"nl_email\">\n      {{lang 'common.newsletter.email'}}\n    </label>\n    <input\n      class=\"form-input newsletter-input\"\n      id=\"nl_email\"\n      name=\"nl_email\"\n      type=\"email\"\n      value=\"{{customer.email}}\"\n      placeholder=\"{{lang 'common.newsletter.placeholder'}}\"\n    >\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n    <button class=\"button button-primary button-newsletter\" type=\"submit\">\n      <svg class=\"newsletter-arrow\" title=\"{{lang 'common.newsletter.heading'}}\" role=\"img\">\n        <use xlink:href=\"#icon-arrow\"></use>\n      </svg>\n    </button>\n  </div>\n</form>\n"}