{"components/faceted-search/facets/hierarchy":"<ul class=\"{{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\"\n    id=\"filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-count=\"{{items_count}}\"\n    data-facet=\"{{facet}}\"\n    data-facet-filter-wrapper\n>\n  {{#each items}}\n    <li class=\"sidebar-filter-item filter-item\" data-facet-item>\n      <a href=\"{{url}}\"\n         class=\"sidebar-filter-item-link {{#if selected}}sidebar-filter-item-link-selected{{/if}}\"\n         data-faceted-search-facet>\n\n        {{title}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}