{"components/cart/totals":"<section class=\"cart-totals\">\n  <div class=\"cart-subtotals\">\n    <div class=\"cart-subtotal-line\">\n      <span class=\"cart-subtotal-label\">{{lang 'cart.subtotal'}}</span>\n      <span class=\"cart-subtotal-content\">{{cart.sub_total.formatted}}</span>\n    </div>\n\n    <div class=\"cart-subtotal-line\">\n      <span class=\"cart-subtotal-label\">{{lang 'cart.shipping_calculator.title'}}</span>\n      {{#if cart.shipping_handling.shipping_cost}}\n        <span class=\"cart-subtotal-content\">\n          {{cart.shipping_handling.shipping_cost.formatted}}\n        </span>\n        <span class=\"cart-shipping-recalculate\">\n          <a\n            class=\"cart-shipping-calculator-toggle\"\n            data-shipping-calculator-toggle\n          >\n            {{lang 'cart.shipping_calculator.recalculate'}}\n          </a>\n        </span>\n      {{else}}\n        <span class=\"cart-subtotal-content\">\n          {{#if cart.shipping_handling.shipping_cost \"===\" 0}}\n            {{#if cart.shipping_handling.provider}}\n              {{cart.shipping_handling.provider.description}}\n            {{/if}}\n          {{else}}\n            <a\n              class=\"cart-shipping-calculator-toggle\"\n              data-shipping-calculator-toggle\n            >\n              {{lang 'cart.shipping_calculator.estimate_shipping'}}\n            </a>\n          {{/if}}\n        </span>\n      {{/if}}\n    </div>\n\n    {{> components/cart/shipping-calculator cart.shipping_handling}}\n\n    {{#each cart.taxes}}\n      <div class=\"cart-subtotal-line\">\n        <span class=\"cart-subtotal-label\">{{name}}</span>\n        <span class=\"cart-subtotal-content\">\n          {{cost.formatted}}\n        </span>\n      </div>\n    {{/each}}\n\n    {{#if cart.coupons}}\n      {{#each cart.coupons}} {{! only one will come from the platform but it's in array format}}\n        <div class=\"cart-subtotal-line\">\n          <span class=\"cart-subtotal-label\">\n            {{lang 'cart.coupon_codes.label' code=code}}\n            <a href=\"{{remove_url}}\">{{lang 'cart.coupon_codes.remove'}}</a>\n          </span>\n          <span class=\"cart-subtotal-content\">{{discount.formatted}}</span>\n        </div>\n      {{/each}}\n    {{/if}}\n\n    {{#each cart.gift_certificates}}\n      <div class=\"cart-subtotal-line\">\n        <span class=\"cart-subtotal-label\">\n          {{lang 'cart.gift_certificates.label' code=code}}\n          {{remaining.formatted}}\n          <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n        </span>\n        <span class=\"cart-subtotal-content\">{{used.formatted}}</span>\n      </div>\n    {{/each}}\n\n    {{#if cart.discount}}\n      <div class=\"cart-subtotal-line\">\n        <span class=\"cart-subtotal-label\">{{lang 'cart.discount'}}</span>\n        <span class=\"cart-subtotal-content\">{{cart.discount.formatted}}</span>\n      </div>\n    {{/if}}\n  </div>\n\n  <div data-coupon-errors></div>\n  <div data-gift-certificate-errors></div>\n\n  {{> components/cart/coupon-codes}}\n  {{> components/cart/gift-certificates}}\n\n  <div class=\"cart-grand-total\">\n    <span class=\"cart-total-label\">{{lang 'cart.grand_total'}}</span>\n    <span class=\"cart-subtotal-content\">\n      {{cart.grand_total.formatted}}\n    </span>\n  </div>\n\n  <div class=\"cart-actions-row\">\n    <a class=\"button button-primary\" href=\"{{urls.checkout.single_address}}\">{{lang 'cart.checkout'}}</a>\n  </div>\n\n  {{#if cart.additional_checkout_buttons.length}}\n    <div class=\"additional-checkout-buttons\">\n      {{#each cart.additional_checkout_buttons}}\n        {{{this}}}\n      {{/each}}\n    </div>\n  {{/if}}\n</section>\n","components/cart/shipping-calculator":"<div class=\"shipping-calculator\" data-shipping-calculator>\n  <form class=\"form\">\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-country\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.select_a_country'}}</span>\n        <span class=\"form-select-wrapper\">\n          <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n            <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n            {{#each countries}}\n              <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                {{name}}\n              </option>\n            {{/each}}\n          </select>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-state\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.select_a_state'}}</span>\n        <span class=\"form-select-wrapper\">\n          <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n            <option>{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n            {{#each states}}\n              <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                {{name}}\n              </option>\n            {{/each}}\n          </select>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-city\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.suburb_city'}}</span>\n        <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\">\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-zip\">\n        <span class=\"form-label-text\">{{lang 'cart.shipping_calculator.zip_postal_code'}}</span>\n        <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\">\n      </label>\n    </div>\n\n    <div class=\"form-actions\">\n      <button class=\"button button-primary\">{{lang 'cart.shipping_calculator.estimate_shipping'}}</button>\n    </div>\n  </form>\n\n  <div data-shipping-quotes></div>\n  <div data-shipping-errors></div>\n</div>\n","components/cart/coupon-codes":"{{inject 'couponCodeEmptyInput' (lang 'cart.coupon_codes.error')}}\n\n<div class=\"cart-coupon-codes\" data-coupon-codes>\n  <form class=\"form form-coupon-code\" method=\"post\" action=\"{{urls.cart}}\" data-coupon-code-form>\n    <input\n      class=\"form-input\"\n      id=\"couponcode\"\n      type=\"text\"\n      name=\"couponcode\"\n      placeholder=\"{{lang 'cart.coupon_codes.enter_code'}}\"\n      data-coupon-code-input\n    >\n    <button class=\"button button-secondary button-coupon-code\" type=\"submit\">\n      <span class=\"cart-button-text\">{{lang 'cart.coupon_codes.submit'}}</span>\n      <svg class=\"cart-button-arrow\" title=\"{{lang 'cart.coupon_codes.submit'}}\" role=\"img\">\n        <use xlink:href=\"#icon-arrow\"></use>\n      </svg>\n    </button>\n    <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n  </form>\n</div>\n","components/cart/gift-certificates":"{{inject 'giftCertificateInputEmpty' (lang 'cart.gift_certificates.error')}}\n\n<div data-gift-certificates>\n  <form class=\"form form-gift-certificate\" method=\"post\" action=\"{{urls.cart}}\" data-gift-certificate-form>\n    <input\n      class=\"form-input\"\n      id=\"couponcode\"\n      type=\"text\"\n      name=\"certcode\"\n      placeholder=\"{{lang 'cart.gift_certificates.enter_code'}}\"\n      data-gift-certificate-input\n    >\n    <button class=\"button button-secondary button-gift-certificate\" type=\"submit\">\n      <span class=\"cart-button-text\">{{lang 'cart.gift_certificates.submit'}}</span>\n        <svg class=\"cart-button-arrow\" title=\"{{lang 'cart.gift_certificates.submit'}}\" role=\"img\">\n          <use xlink:href=\"#icon-arrow\"></use>\n        </svg>\n    </button>\n    <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n  </form>\n</div>\n"}